main
{
	int x = 50;
	int y = 10;

	printf (x / y); // 5

	x = 1
	y = 2
	printf (x | y); // or lógico. Salida = 3

	x = 15
	y = 10
	printf (x & y); // and lógico. Salida = 10

	printf (x == y) // false

	x = 10;
	
	printf (x == y) // true
	printf (x != y) // false

	x = 15
	printf (x != y) // true


	// x=15 y = 10 en este momento
	printf (x <= y) // false
	y = 333;
	printf (x <= y) // true

	//x=15 y = 333
	printf (x >= y) // false
	y = 2
	printf (x >= y) // true

	// x=15 y = 2 en este momento
	printf (x < y) // false
	y = 333;
	printf (x < y) // true

	//x=15 y = 333 en este momento
	printf (x > y) // false
	y = 2
	printf (x > y) // true
}
